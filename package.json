{
  "name": "vue2-easy-controls",
  "version": "1.0.1",
  "description": "An easy to use Checkbox and Radio components for Vue 2",
  "author": {
    "name": "umutbozdag",
    "email": "umutbozdag@outlook.com.tr"
  },
  "private": false,
  "main": "dist/vue-easy-controls.ssr.js",
  "browser": "dist/vue-easy-controls.esm.js",
  "module": "dist/vue-easy-controls.esm.js",
  "unpkg": "dist/vue-easy-controls.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/umutbozdag/vue-easy-controls.git"
  },
  "keywords": ["checkbox", "radio", "vue", "component", "easy", "custom"],
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js --open",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "cross-env": "^7.0.2",
    "minimist": "^1.2.5",
    "node-sass": "^4.14.1",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "sass-loader": "^9.0.2",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "engines": {
    "node": ">=10"
  }
}
